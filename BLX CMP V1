local script1 = [[
  loadstring(game:HttpGet("https://raw.githubusercontent.com/MRHafiz786/fruit-35-nm/main/fruit%20nm"))();
]]

local script2 = [[
  loadstring(game:HttpGet("https://raw.githubusercontent.com/MRHafiz786/fruit-spwn-show/main/fruit%20spaw"))();
]]

local combinedScript = script1 .. script2

-- Execute the combined script
loadstring(combinedScript)()
