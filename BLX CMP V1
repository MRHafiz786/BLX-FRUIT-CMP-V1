local script1 = [[
  loadstring(game:HttpGet("https://raw.githubusercontent.com/MRHafiz786/FRUIT-SPAWN-ESP/main/FRUIT%20SPAWN%20NAMES"))();
]]

local script2 = [[
  loadstring(game:HttpGet("https://raw.githubusercontent.com/MRHafiz786/FRUIT-35-NAMES-ESP/main/FRUIT%2035%20NAMES%20SCRIPT"))();
]]

local combinedScript = script1 .. script2

-- Execute the combined script
loadstring(combinedScript)()
